a[0].org_size;\n\n        }\n            \n        })\n        .catch((error) => {\n            console.error(error);\n        });\n    },\n    logoutfn(){\n            localStorage.clear()\n            this.$router.push('/')\n        },\n    \n},\ncreated() {\n            this.token = localStorage.getItem('authToken');\n            this.userid = localStorage.getItem('user_id');\n\n            if (!this.token) {\n                this.$router.push('/')\n            }\n            this.roles = localStorage.getItem('roles');\n            if (this.roles=='sponsor'){\n                this.userid = localStorage.getItem('user_id');\n            }\n            else if (this.roles =='admin'){\n                this.userid = this.$route.params.id;\n            }\n\n            this.viewSponsorProfile() ;\n        },\n}\n</script>\n<style lang=\"\">\n    \n</style>"],"mappings":"AAsBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EAEXC,IAAI,EAAE,iBAAiB;EACzBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACHC,MAAK,EAAI,EAAE;MACXC,QAAQ,EAAC,EAAE;MACXC,QAAQ,EAAC,EAAE;MACXC,UAAU,EAAC,EAAE;MACbC,QAAQ,EAAC,IAAI;MACbC,KAAK,EAAE,IAAI;MACXC,KAAK,EAAC,EAAE;MACRC,MAAM,EAAC;IACP;EACN,CAAC;EAEDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACjBZ,KAAI,CACHa,GAAG,CAAC,6CAA6C,IAAI,CAACH,MAAM,EAAE,EAC/D;QAACI,OAAO,EAAE;UAACC,aAAa,EAAE,GAAG,IAAI,CAACP,KAAK;QAAE;MAAC,CAC1C,EACCQ,IAAI,CAAEC,QAAQ,IAAK;QACpBC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEF,QAAQ;QACtC,IAAIA,QAAQ,CAACG,MAAK,IAAK,GAAG,EAAE;UACxB,IAAI,CAACC,KAAI,GAAIJ,QAAQ,CAACf,IAAI;UAC1B,IAAI,CAACC,MAAK,GAAIc,QAAQ,CAACf,IAAI,CAACA,IAAI,CAAC,CAAC,CAAC,CAACC,MAAM;UAC1C,IAAI,CAACC,QAAO,GAAIa,QAAQ,CAACf,IAAI,CAACA,IAAI,CAAC,CAAC,CAAC,CAACE,QAAQ;UAC9C,IAAI,CAACC,QAAO,GAAIY,QAAQ,CAACf,IAAI,CAACA,IAAI,CAAC,CAAC,CAAC,CAACG,QAAQ;UAC9C,IAAI,CAACC,UAAS,GAAIW,QAAQ,CAACf,IAAI,CAACA,IAAI,CAAC,CAAC,CAAC,CAACI,UAAU;UAClD,IAAI,CAACC,QAAO,GAAIU,QAAQ,CAACf,IAAI,CAACA,IAAI,CAAC,CAAC,CAAC,CAACK,QAAQ;QAElD;MAEA,CAAC,EACAe,KAAK,CAAEC,KAAK,IAAK;QACdL,OAAO,CAACK,KAAK,CAACA,KAAK,CAAC;MACxB,CAAC,CAAC;IACN,CAAC;IACDC