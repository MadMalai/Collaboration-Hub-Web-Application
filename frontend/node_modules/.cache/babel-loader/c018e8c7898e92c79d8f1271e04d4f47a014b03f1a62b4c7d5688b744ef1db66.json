y,\n        },\n        {headers: {Authorization: `${this.token}`}})\n        .then((response) => {\n        console.log(\"response block\", response)\n        if (response.status == 201) {\n           this.var12 = response.data;\n           this.cam_name='',\n           this.cam_desc='',\n           this.cam_category='',\n           this.cam_start_date='',\n           this.cam_end_date='',\n           this.cam_budget='',\n           this.goals='',\n           this.visibility='',\n           this.$router.push({\n                    name: 'viewCampaignDetail', \n                    params:{cam_id: this.id}\n                });\n        }\n          \n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    }\n\n  },\n\n  created() {\n    this.token = localStorage.getItem('authToken');\n    this.id = this.$route.params.cam_id;\n\n    if (!this.token) {\n        this.$router.push('/')\n    }\n    this.roles = localStorage.getItem('roles');\n\n    this.viewCampaigns() ;\n  },\n\n    \n}\n</script>\n<style lang=\"\">\n    \n</style>"],"mappings":"AAsDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,eAAe;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAC,EAAE;MACXC,QAAQ,EAAC,EAAE;MACXC,YAAY,EAAC,EAAE;MACfC,cAAc,EAAC,EAAE;MACjBC,YAAY,EAAC,EAAE;MACfC,UAAU,EAAC,EAAE;MACbC,KAAK,EAAC,EAAE;MACRC,UAAU,EAAC,EAAE;MACbC,MAAM,EAAC,IAAI;MACXC,KAAK,EAAE,IAAI;MACXC,KAAK,EAAC;IACJ;EACN,CAAC;EACDC,OAAO,EAAE;IAEPC,aAAaA,CAAA,EAAG;MACZf,KAAI,CACHgB,GAAG,CAAC,8CAA8C,IAAI,CAACC,EAAE,EAAE,EACpD;QAACC,OAAO,EAAE;UAACC,aAAa,EAAE,GAAG,IAAI,CAACP,KAAK;QAAE;MAAC,CAAC,EAClDQ,IAAI,CAAEC,QAAQ,IAAK;QACpBC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEF,QAAQ;QACtC,IAAIA,QAAQ,CAACG,MAAK,IAAK,GAAG,EAAE;UACzB,IAAI,CAACC,KAAI,GAAIJ,QAAQ,CAACnB,IAAI,CAACA,IAAI;UAC/B,IAAI,CAACC,QAAQ,GAACkB,QAAQ,CAACnB,IAAI,CAACA,IAAI,CAAC,CAAC,CAAC,CAACC,QAAQ,EAC5C,IAAI,CAACC,QAAQ,GAACiB,QAAQ,CAACnB,IAAI,CAACA,IAAI,CAAC,CAAC,CAAC,CAACE,QAAQ,EAC5C,IAAI,CAACC,YAAY,GAACgB,QAAQ,CAACnB,IAAI,CAACA,IAAI,CAAC,CAAC,