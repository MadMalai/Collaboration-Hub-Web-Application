{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'create_ad',\n  data() {\n    return {\n      cam_id: null,\n      ad_name: '',\n      ad_req: '',\n      terms: '',\n      status: '',\n      pref_inf_category: '',\n      ad_budget: null\n    };\n  },\n  methods: {\n    AddAdDetails() {\n      axios.post('http://127.0.0.1:5000/api/AdResource', {\n        cam_id: this.id,\n        ad_name: this.ad_name,\n        ad_req: this.ad_req,\n        terms: this.terms,\n        pref_inf_category: this.pref_inf_category,\n        ad_budget: this.ad_budget,\n        status: \"pending\"\n      }, {\n        headers: {\n          Authorization: `${this.token}`\n        }\n      }).then(response => {\n        console.log(\"response block\", response);\n        if (response.status == 201) {\n          this.$router.push({\n            name: 'viewCampaignDetail',\n            params: {\n              cam_id: this.id\n            }\n          });\n        }\n      }).catch(error => {\n        console.log(\"error block\", error);\n      });\n    },\n    cancelEditAd() {\n      this.$router.push({\n        name: 'viewCampaignDetail',\n        params: {\n          cam_id: this.cam_id\n        }\n      });\n    }\n  },\n  created() {\n    this.token = localStorage.getItem('authToken');\n    this.id = this.$route.params.cam_id;\n    if (!this.token) {\n      this.$router.push('/');\n    }\n    this.roles = localStorage.getItem('roles');\n  }\n};","map":{"version":3,"names":["axios","name","data","cam_id","ad_name","ad_req","terms","status","pref_inf_category","ad_budget","methods","AddAdDetails","post","id","headers","Authorization","token","then","response","console","log","$router","push","params","catch","error","cancelEditAd","created","localStorage","getItem","$route","roles"],"sources":["/Users/meena/Documents/learning/annamalai/Azure Test App/Collaboration-Hub-Web-Application/frontend/src/components/create_ad.vue"],"sourcesContent":["<template >\n    <div>\n        \n  <div class=\"container mt-5\">\n    <h2>Create Ad</h2>\n    <form >\n        <div class=\"form-group mb-3\">\n            <label for=\"ad_name\">Ad name</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"this.ad_name\" required>\n        </div>\n        <div class=\"form-group mb-3\">\n            <label for=\"ad_req\">Ad Requirement</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"this.ad_req\" required>\n        </div>\n        <div class=\"form-group mb-3\">\n            <label for=\"terms\">Terms</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"this.terms\" required>\n        </div>\n        <div class=\"form-group mb-3\">\n            <label for=\"ad_budget\">Budget</label>\n            <input type=\"number\" class=\"form-control\" v-model=\"this.ad_budget\" required>\n        </div>    \n        <div class=\"form-group mb-3\"> \n          <input type=\"hidden\" value=\"Pending\"  v-model=\"this.status\">\n        </div>\n        \n        <div class=\"form-group mb-3\">\n            <label for=\"pref_inf_category\">Preferred Influenecer Category</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"this.pref_inf_category\" required>\n        </div>\n\n            <div class=\"mb-3\"></div>\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"AddAdDetails\">Add</button>\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"cancelEditAd\">Cancel</button>\n        </form>\n</div>\n</div>\n</template>\n<script>\nimport axios from 'axios';\n\nexport default {\n    \n    name: 'create_ad',\n  data() {\n    return {\n        cam_id: null,\n        ad_name: '',\n        ad_req: '',\n        terms: '',\n        status: '',\n        pref_inf_category: '',\n        ad_budget: null\n    };\n  },\n  methods: {\n    \n    AddAdDetails() {\n    axios\n        .post('http://127.0.0.1:5000/api/AdResource', \n        {\n        cam_id: this.id,\n        ad_name: this.ad_name,\n        ad_req: this.ad_req,\n        terms: this.terms,\n        pref_inf_category: this.pref_inf_category,\n        ad_budget: this.ad_budget,\n        status: \"pending\",\n        },\n        {headers: {Authorization: `${this.token}`}})\n        .then(response => {\n            console.log(\"response block\", response)\n            if (response.status == 201) {\n                this.$router.push({\n                    name: 'viewCampaignDetail', \n                    params:{cam_id: this.id}\n                });\n            }\n        })\n        .catch(error => {\n            console.log(\"error block\", error)\n        })\n    },\n    cancelEditAd(){\n        this.$router.push({\n                    name: 'viewCampaignDetail', \n                    params:{cam_id: this.cam_id}\n                });\n    }\n  },\n  created() {\n    this.token = localStorage.getItem('authToken');\n    this.id = this.$route.params.cam_id;\n\n    if (!this.token) {\n        this.$router.push('/')\n    }\n    this.roles = localStorage.getItem('roles');\n  },\n}\n</script>\n<style lang=\"\">\n    \n</style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EAEXC,IAAI,EAAE,WAAW;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACHC,MAAM,EAAE,IAAI;MACZC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,iBAAiB,EAAE,EAAE;MACrBC,SAAS,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IAEPC,YAAYA,CAAA,EAAG;MACfX,KAAI,CACCY,IAAI,CAAC,sCAAsC,EAC5C;QACAT,MAAM,EAAE,IAAI,CAACU,EAAE;QACfT,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBE,iBAAiB,EAAE,IAAI,CAACA,iBAAiB;QACzCC,SAAS,EAAE,IAAI,CAACA,SAAS;QACzBF,MAAM,EAAE;MACR,CAAC,EACD;QAACO,OAAO,EAAE;UAACC,aAAa,EAAE,GAAG,IAAI,CAACC,KAAK;QAAE;MAAC,CAAC,EAC1CC,IAAI,CAACC,QAAO,IAAK;QACdC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEF,QAAQ;QACtC,IAAIA,QAAQ,CAACX,MAAK,IAAK,GAAG,EAAE;UACxB,IAAI,CAACc,OAAO,CAACC,IAAI,CAAC;YACdrB,IAAI,EAAE,oBAAoB;YAC1BsB,MAAM,EAAC;cAA