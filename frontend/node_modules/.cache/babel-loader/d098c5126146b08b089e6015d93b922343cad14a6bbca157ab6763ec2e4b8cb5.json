{"ast":null,"code":"import axios from 'axios';\nimport { Chart } from 'chart.js';\nexport default {\n  name: 'admin_stat',\n  data() {\n    return {\n      kpi_data: {\n        spon_count: 0,\n        inf_count: 0,\n        cam_count: 0,\n        ad_count: 0\n      }\n    };\n  },\n  methods: {\n    fetchKpiData() {\n      const path = 'http://127.0.0.1:5000/api/AdminStat';\n      axios.get(path, {\n        headers: {\n          Authorization: `${this.token}`\n        }\n      }).then(response => {\n        if (response.status === 200) {\n          this.kpi_data = response.data.kpi_data;\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  },\n  created() {\n    this.token = localStorage.getItem('authToken');\n    if (!this.token) {\n      this.$router.push('/');\n    }\n    this.fetchKpiData();\n  }\n};","map":{"version":3,"names":["axios","Chart","name","data","kpi_data","spon_count","inf_count","cam_count","ad_count","methods","fetchKpiData","path","get","headers","Authorization","token","then","response","status","catch","error","console","created","localStorage","getItem","$router","push"],"sources":["/Users/meena/Documents/learning/annamalai/Azure Test App/Collaboration-Hub-Web-Application/frontend/src/components/admin_stat.vue"],"sourcesContent":["<template>\n    <div>\n      <h3>Admin Statistics</h3>\n      <br>\n      <div class=\"container\">\n        <div class=\"row align-items-start\">\n          <div class=\"row\">\n            <!-- Sponsors KPI -->\n            <div class=\"col-md-6\">\n              <div class=\"kpi-box\">\n                <div class=\"kpi-title\">Sponsors</div>\n                <div class=\"kpi-value\">{{ kpi_data.spon_count }}</div>\n              </div>\n            </div>\n            <!-- Influencers KPI -->\n            <div class=\"col-md-6\">\n              <div class=\"kpi-box\">\n                <div class=\"kpi-title\">Influencers</div>\n                <div class=\"kpi-value\">{{ kpi_data.inf_count }}</div>\n              </div>\n            </div>\n            <!-- Campaigns KPI -->\n            <div class=\"col-md-6\">\n              <div class=\"kpi-box\">\n                <div class=\"kpi-title\">Campaigns</div>\n                <div class=\"kpi-value\">{{ kpi_data.cam_count }}</div>\n              </div>\n            </div>\n            <!-- Advertisements KPI -->\n            <div class=\"col-md-6\">\n              <div class=\"kpi-box\">\n                <div class=\"kpi-title\">Advertisements</div>\n                <div class=\"kpi-value\">{{ kpi_data.ad_count }}</div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>   \n</template>\n\n  <script>\n  import axios from 'axios';\n  import { Chart } from 'chart.js';\n  \n  export default {\n    name: 'admin_stat',\n    data() {\n      return {\n        kpi_data: {\n          spon_count: 0,\n          inf_count: 0,\n          cam_count: 0,\n          ad_count: 0\n        },\n      };\n    },\n    methods: {\n      fetchKpiData() {\n        const path = 'http://127.0.0.1:5000/api/AdminStat';\n        axios\n          .get(path, {\n            headers: { Authorization: `${this.token}` },\n          })\n          .then((response) => {\n            if (response.status === 200) {\n              this.kpi_data = response.data.kpi_data;\n            }\n          })                                                                                                                          \n          .catch((error) => {\n            console.error(error);\n          });\n    },\n  },\n  created() {\n    this.token = localStorage.getItem('authToken');\n    if (!this.token) {\n      this.$router.push('/');\n    }\n    this.fetchKpiData();\n  },\n  };\n  </script> \n      \n  <style scoped>\n    .kpi-box {\n    padding: 20px;\n    background-color: #9669ea;\n    border-radius: 8px;\n    margin-bottom: 20px;\n    text-align: center;\n  }\n  \n  .kpi-title {\n