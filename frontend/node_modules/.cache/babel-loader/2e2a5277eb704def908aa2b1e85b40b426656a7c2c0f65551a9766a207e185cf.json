                        <select type=\"text\" class=\"form-select\" v-model=this.visibility >\n                            <option value=\"Public\">Public</option>\n                            <option value=\"Private\">Private</option>\n                        </select>\n                    </div>\n                    <div class=\"form-group\">\n                        <label for=\"cam_category\">Campaign Category</label>\n                        <input type=\"text\" class=\"form-control\" v-model=this.cam_category >\n                    </div>\n                    <div class=\"form-group\">\n                        <label for=\"goals\">Goals</label>\n                        <input type=\"text\" class=\"form-control\" v-model=this.goals >\n                    </div>\n                    <button class=\"btn btn-primary\" @click=\"create_campaign\" >Save</button>\n                    <button type=\"button\" class=\"btn btn-primary\" @click=\"cancelCreateCampaign\">Cancel</button>\n                </div>\n                </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\nimport axios from 'axios';\n\nexport default {\n    name: 'create_campaign',\n  data() {\n    return {\n      cam_name:'',\n      cam_desc:'',\n      cam_category:'',\n      cam_start_date:'',\n      cam_end_date:'',\n      cam_budget:'',\n      goals:'',\n      visibility:'',\n      cam_id:null,\n      token: null,\n      roles:[],\n        }\n  },\n  methods: {\n    \n    create_campaign(){\n        axios\n        .post('http://127.0.0.1:5000/api/CampaignResource',\n        {\n            cam_name: this.cam_name,\n            cam_desc:this.cam_desc,\n            cam_category:this.cam_category,\n            cam_start_date:this.cam_start_date,\n            cam_end_date:this.cam_end_date,\n            cam_budget:this.cam_budget,\n            goals:this.goals,\n            visibility: this.visibility,\n        },\n        {headers: {Authorization: `${this.token}`}})\n        .then((response) => {\n        console.log(\"response block\", response)\n        if (response.status == 201) {\n           this.var12 = response.data;\n           this.cam_name='',\n           this.cam_desc='',\n           this.cam_category='',\n           this.cam_start_date='',\n           this.cam_end_date='',\n           this.cam_budget='',\n           this.goals='',\n           this.visibility='',\n           this.$router.push('/campaign_home');\n        }  \n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n    logoutfn(){\n        localStorage.clear()\n        this.$router.push('/')\n    },\n    cancelCreateCampaign(){\n        this.$router.push('/campaign_home')\n    }\n    \n    },\n    created() {\n        this.token = localStorage.getItem('authToken');\n\n        if (!this.token) {\n            this.$router.push('/')\n        }\n        this.roles = localStorage.getItem('roles');\n    },\n}\n</script>\n<style lang=\"\">\n    \n</style>"],"mappings":"AAuDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACzBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAC,EAAE;MACXC,QAAQ,EAAC,EAAE;MACXC,YAAY,EAAC,EAAE;MACfC,cAAc,EAAC,EAAE;MACjBC,YAAY,EAAC,EAAE;MACfC,UAAU,EAAC,EAAE;MACbC,KAAK,EAAC,EAAE;MACRC,UAAU,EAAC,EAAE;MACbC,MAAM,EAAC,IAAI;MACXC,KAAK,EAAE,IAAI;MACXC,KAAK,EAAC;IACJ;EACN,CAAC;EACDC,OAAO,EAAE;IAEPC,eAAeA,CAAA,EAAE;MACbf,KAAI,CACHgB,IAAI,CAAC,4CAA4C,EAClD;QACIb,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,QAAQ,EAAC,IAAI,CAACA,QAAQ;QACtBC,YAAY,EAAC,IAAI,CAACA,YAAY;QAC9BC,cAAc,EAAC,IAAI,CAACA,cAAc;QAClCC,YAAY,EAAC,IAAI,CAACA,YAAY;QAC9BC,UAAU,EAAC,IAAI,CAACA,UAAU;QAC1BC,KAAK,EAAC,IAAI,CAACA,KAAK;QAChBC,UAAU,EAAE,IAAI,CAACA;MACrB,CAAC,EACD;QAACO,OAAO,EAAE;UAACC,aAAa,EAAE,GAAG,IAAI,CAACN,KAAK;QAAE;MAAC,CAAC,EAC1CO,IAAI,CAAEC,QAAQ,IAAK;QACpBC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEF,QAAQ;QACtC,IAAIA,QAAQ,CAACG,MAAK,IAAK,GAAG,EAAE;UACzB,IAAI,CAACC,KAAI,GAAIJ,QAAQ,CAAClB,IAAI;UAC1B,IAAI,CAACC,QAAQ,GAAC,EAAE,EAChB